<!DOCTYPE html>
<html>
<head>
	<title>Unsubscibe Form</title>
	<meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0">
	<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
	<link href="https://fonts.googleapis.com/css?family=Montserrat:400,500,700&display=swap" rel="stylesheet">
	<link rel="stylesheet" href="<%= baseurl() %>css/style.css">
</head>
<body>
<div class="wrapper">
	<div class="title-unsubs">
		<h2>
			<!-- UNSUBSCRIBE FORM -->
		</h2>
	</div>
	<div class="containt" ng-app="xlsApp" ng-controller="xlsCtrl">
		<div>
			<input type="text" name="search" ng-model="search">
		</div>
		<table style="width: 100%;" class="full-tables" cellspacing="0"cellpadding="0">
            <tr>
                <td ng-click="header('Head1')" style="cursor: pointer;">ID</td>
                <td ng-click="header('Head2')" style="cursor: pointer;">Merek Mobil</td>
                <td ng-click="header('Head3')" style="cursor: pointer;">Tipe Mobil</td>
            </tr>
            <tr ng-repeat="result in data | orderBy: headerBy | filter: search">
                <td>{{result.ID}}</td>
                <td>{{result.merek}}</td>
                <td>{{result.tipe_mobil}}</td>
            </tr>
		</table>
		<div style="float: right; margin-top: 20px;">
			<a href="" class="myButton-submit" style="display: inline-block;">Cancel</a>
			<a href="" class="myButton-submit" style="display: inline-block;">Save & Continue</a>
		</div>
	</div>
</div>
</body>
</html>

<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/angularjs/1.6.9/angular.min.js"></script>
<script>
var dataJSON = <%- JSON.stringify(jsonxls) %>
var app = angular.module("xlsApp", []);
app.controller("xlsCtrl", function($scope){
	$scope.data = dataJSON,
	$scope.header = function(x){
		$scope.headerBy = x
	}
})
</script>